{
  "title": "iTerm rules",
  "rules": [
    {
      "description": "Ctrl + Alt + T (Open iTerm)",
      "manipulators": [
        {
          "from": {
            "key_code": "t",
            "modifiers": {
              "mandatory": [
                "control",
                "option"
              ],
              "optional": [
              ]
            }
          },
          "to": [
            {
              "shell_command": "open -a iTerm"
            }
          ],
          "type": "basic"
        }
      ]
    }
  ]
}